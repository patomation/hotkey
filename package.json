{
  "name": "@patomation/hotkey",
  "version": "1.6.0",
  "description": "Set hotkeys for web apps",
  "public": true,
  "main": "lib/index.esm.js",
  "module": "lib/index.esm.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "prepublish": "npm run build",
    "build": "rollup -c",
    "lint": "eslint --ext .ts .",
    "test": "mocha -r ts-node/register test/test.ts",
    "coverage": "nyc mocha -r ts-node/register test/test.ts"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "npm run lint && npm run test"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/patomation/hotkey.git"
  },
  "keywords": [
    "hotkeys",
    "control",
    "command",
    "shift",
    "hotkey",
    "modifiers"
  ],
  "author": "Patrick Kelly",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/patomation/hotkey/issues"
  },
  "homepage": "https://github.com/patomation/hotkey#readme",
  "devDependencies": {
    "@commitlint/config-conventional": "^9.0.1",
    "@types/chai": "^4.2.11",
    "@types/mocha": "^7.0.2",
    "@types/node": "^14.0.18",
    "@typescript-eslint/eslint-plugin": "^3.6.0",
    "chai": "^4.2.0",
    "commitlint": "^9.0.1",
    "coveralls": "^3.1.0",
    "eslint": "^7.4.0",
    "eslint-config-standard-with-typescript": "^18.0.2",
    "eslint-plugin-chai": "0.0.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-mocha": "^7.0.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^4.2.5",
    "jsdom": "^16.2.2",
    "jsdom-global": "^3.0.2",
    "mocha": "^8.0.1",
    "nyc": "^15.1.0",
    "rollup": "^2.20.0",
    "rollup-plugin-typescript2": "^0.27.1",
    "ts-node": "^8.10.2",
    "tsconfigs": "^5.0.0",
    "typescript": "^3.9.6"
  },
  "dependencies": {}
