{
  "name": "@patomation/hotkey",
  "version": "1.5.4",
  "description": "Set hotkeys for web apps",
  "public": true,
  "type": "module",
  "main": "lib/index.cjs.js",
  "module": "lib/index.esm.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "prepublish": "npm run build",
    "build": "rollup -c",
    "lint": "eslint --ext .js,.ts .",
    "test": "mocha -r ts-node/register test/test.ts",
    "coverage": "nyc mocha -r ts-node/register test/test.ts"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "npm run lint && npm run test"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/patomation/hotkey.git"
  },
  "keywords": [
    "hotkeys",
    "control",
    "command",
    "shift",
    "hotkey",
    "modifiers"
  ],
  "author": "Patrick Kelly",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/patomation/hotkey/issues"
  },
  "homepage": "https://github.com/patomation/hotkey#readme",
  "devDependencies": {
    "@commitlint/config-conventional": "^8.3.4",
    "@types/chai": "^4.2.9",
    "@types/mocha": "^7.0.1",
    "@types/node": "^13.7.6",
    "@typescript-eslint/eslint-plugin": "^2.21.0",
    "chai": "^4.2.0",
    "commitlint": "^8.3.5",
    "coveralls": "^3.0.5",
    "eslint": "^6.8.0",
    "eslint-config-standard-with-typescript": "^14.0.0",
    "eslint-plugin-chai": "0.0.1",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-mocha": "^6.3.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^4.2.3",
    "jsdom": "^15.1.1",
    "jsdom-global": "^3.0.2",
    "mocha": "^6.1.4",
    "nyc": "^15.0.0",
    "rollup": "^1.31.1",
    "rollup-plugin-typescript2": "^0.26.0",
    "ts-node": "^8.6.2",
    "tsconfigs": "^4.0.2",
    "typescript": "^3.8.2"
  },
  "dependencies": {}
}
